<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="daffodil.international.ac.coopapplication">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CoOpMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".StudentSignUpActivity"/>
        <activity android:name=".SignUpAsActivity"/>
        <activity android:name=".UniversitySignUpActivity">
        </activity>
        <activity android:name=".EmployerSignUpActivity">
        </activity>
        <activity android:name=".SignUpFeedback">
        </activity>
    </application>

</manifest>